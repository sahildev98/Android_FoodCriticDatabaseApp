<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfoodcriticapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFoodCriticApp">
        <activity android:name=".ViewFoodItemsActivity"/>
        <activity android:name=".EditFoodActivity" />
        <activity android:name=".EditRestaurantActivity" />
        <activity android:name=".ViewRestaurantActivity" />
        <activity android:name=".ListRestaurantActivity" />
        <activity android:name=".OutputFoodCrticActivity" />
        <activity android:name=".AddReviewActivity" />
        <activity android:name=".AddRestaurantActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".AddFoodActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>